marklist = []
absent_count = 0

n = int(input("Enter numbaer of students : "))

for i in range(n):
    mark = int(input(f"Enter marks of {i+1} student (Enter -1 for absent) : "))
    if mark == -1:
        absent_count += 1
    else:
        marklist.append(mark)

print(marklist)
print(absent_count)

sum=0

min_value = marklist[0]
max_value = marklist[0]


for mark in marklist :
    if mark in marklist:
        sum += mark

        if mark < min_value:
            min_value = mark
        if mark > max_value:
            max_value = mark


def most_frequent_integer(marklist):
    if not marklist:
        return None

    frequency = {}
    for mark in marklist:
        if mark in frequency:
            frequency[mark] += 1
        else:
            frequency[mark] = 1

    most_frequent_num = None
    max_count = 0
    for mark, count in frequency.items():
        if count > max_count:
            most_frequent_num = mark
            max_count = count

    return most_frequent_num



print(sum)
print(f"a) The average score of class :{sum/n}")
print(f"b) highest score = {max_value} and lowest score = {min_value}")
print(f"c) Count ofstudents who were absent for the test : {absent_count} ")
print(f"d) Mark with highest frequency : {most_frequent_integer(marklist)} ")
