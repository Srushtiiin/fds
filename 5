marks = []

n= int(input("Enter no of Students in SE :"))
for i in range (n):
    i=float(input(f"Mark of {i+1} student :"))
    marks.append(i)

print(marks)

def insertion_sort(marks):
    for i in range (1,n):
        key = marks[i]
        j = i-1

        while j >= 0 and  key < marks[j]:
            marks[j+1] = marks[j]
            j -= 1
        marks[j+1] = key
    return marks

print("a) Marks after insertion sort :", insertion_sort(marks))


def shell_sort(marks):
    n=len(marks)
    gap = n // 2

    while gap > 0:
        for i in range (gap, n):
            temp = marks[i]
            j = i
            while j >= gap and marks[j - gap] > temp:
                marks[j] = marks [j - gap]
                j -= gap
            marks[j] = temp
        gap //= 2
    return marks

sorted_list = shell_sort(marks)
print("b) Marks after shell sort :", sorted_list)


top_5_elements = sorted_list [-5:]
print("top 5 marks are :", top_5_elements)
